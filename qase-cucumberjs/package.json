  },
  "author": "Parviz Khavari <havaripa@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@cucumber/messages": "^22.0.0",
    "qase-javascript-commons": "^2.0.0"
  },
  "peerDependencies": {
    "@cucumber/cucumber": ">=7.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
