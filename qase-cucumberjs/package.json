{
  "name": "cucumberjs-qase-reporter",
  "version": "2.0.0",
  "description": "Qase TMS CucumberJS Reporter",
  "homepage": "https://github.com/qase-tms/qase-javascript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./reporter": "./dist/reporter.js",
    "./package.json": "./package.json"
